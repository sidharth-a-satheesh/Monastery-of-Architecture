<div class="container">

  <div class="blank" style="height: 129px;"></div>

  <h2 class="mb-5">Add Blog</h2>

  <form action="/blogs" method="post" enctype="multipart/form-data">
    <div>
      <input name="title" type="text" placeholder="Title" required>
    </div>
    <div>
      <input type="date" name="date" class="mt-4" placeholder="Date" required>
    </div>
    <div>
      <textarea name="content" class="mt-4" id="" cols="150" rows="10" placeholder="Content" required></textarea>
    </div>
    <div>
      <label for="">Thumbnail: </label>
      <input type="file" name="thumbnail" required>
    </div>
    <div>
      <label for="">Image: </label>
      <input type="file" name="img" required>
    </div>
    <div class=" text-left mt-4">
      <a href=""><button type="submit" class="btn btn-secondary pl-5 pr-5">Submit</button></a>
    </div>
  </form>
</div>

